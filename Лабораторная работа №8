    cin >> a;
    while (a>0){
        c=a%10;
        b=b*10+c;
        a=a/10;
    }

#include <iostream> //Задание№5

using namespace std;

int main()
{
    int a,b=1,c;
    cin >> a;
    while (b<=a) {
        b=b*10;
    }
    b=b/10;
    c=a/b;
    a=(a%b)*10+c;
    
    cout << a;
    return 0;
}
