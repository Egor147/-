#include <iostream> //Задание №1

using namespace std;

int main()
{
    double a;
    cin >> a;
    int b=a%1024;
    cout >> b;

    return 0;
}

#include <iostream> //Задание №2

using namespace std;

int main()
{
    double a,b;
    cin >> a >> b;
    int c=a/b;
    cout << c;

    return 0;
}

