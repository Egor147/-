#include <iostream> //Задание №1

using namespace std;

int main()
{
    int a,b;
    cin >> b;
    cout << b%60;
    return 0;
}

#include <iostream> //Задание №2

using namespace std;

int main()
{
    string dn [7]= {"Понедельник", "Вторник", "Среда", "Четверг", "Пятница", "Суббота", "Воскресенье"};
    int k;
    cin >> k;
    
    cout << dn[(k%7)-1];
    
    return 0;
} 

#include <iostream> //Задание №3

using namespace std;

int main()
{
    string dn [7]= {"Понедельник", "Вторник", "Среда", "Четверг", "Пятница", "Суббота", "Воскресенье"};
    int k,n;
    cin >> k >>n;
    
    cout << dn[k%7+n-2];
    
    return 0;
}  

#include <iostream> //Задание №4

using namespace std;

int main()
{
    int a,b,c,sp,sk;
    cin >> a >> b >> c;
    sp=a*b;
    sk=c*c;
    cout <<"Количество квадратов=" << sp/sk << endl;
    cout << "Незанятая площадь= " << sp%sk;


    return 0;
}

#include <iostream> //Задание №5

using namespace std;

int main()
{
    int a;
    cin >>a;
    cout << (a/100)+1;
    
    return 0;
}
